{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "typescriptHero.imports.stringQuoteStyle": "'",
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.grouping": [
      "/^@angular/",
      "/^rxjs/",
      "/environment$/",
      "/module$/",
      "/routing$/",
      "/guard$/",
      "/service$/",
      "/directive$/",
      "/pipe$/",
      "/component$/",
      "/model$/",
      "/enum$/",
      "Modules",
      "Plains",
      "Workspace",
      "Remaining"
  ]
}
